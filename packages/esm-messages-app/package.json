{
  "name": "@ehospital/esm-messages-app",
  "version": "1.1.4",
  "description": "Patient flags microfrontend for the OpenMRS SPA",
  "browser": "dist/ehospital-esm-patient-flags-app.js",
  "main": "src/index.ts",
  "source": true,
  "license": "MPL-2.0",
  "homepage": "https://github.com/palladiumkenya/kenyaemr-esm-core#readme",
  "scripts": {
    "start": "openmrs develop",
    "serve": "webpack serve --mode=development",
    "debug": "npm run serve",
    "build": "webpack --mode production",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src --ext ts,tsx",
    "typescript": "tsc",
    "extract-translations": "i18next 'src/**/*.component.tsx' 'src/index.ts' --config ../../tools/i18next-parser.config.js",
    "test": "cross-env TZ=UTC jest --config jest.config.js --verbose false --passWithNoTests",
    "test:watch": "cross-env TZ=UTC jest --watch --config jest.config.js",
    "coverage": "yarn test --coverage"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "keywords": [
    "openmrs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IntelliSOFT-Consulting/eHospital3.x-Apps#readme"
  },
  "bugs": {
    "url": "https://github.com/IntelliSOFT-Consulting/eHospital3.x-Apps/issues"
  },
  "dependencies": {},
  "peerDependencies": {
    "@carbon/react": "1.x",
    "@openmrs/esm-framework": "6.x",
    "react": "^18.1.0",
    "react-i18next": "11.x",
    "react-router-dom": "6.x",
    "swr": "2.x"
  },
  "gitHead": "56cbe9927bf1f9f26c523db5d86597e5b6e12a25"
}
